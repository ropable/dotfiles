[user]
    email = ashley@ropable.com
    name = Ashley Felton
[color]
    ui = true
[core]
    editor = vim
    attributesfile = ~/.gitattributes
[merge]
    tool = vimdiff
    conflictstyle = diff3
[mergetool]
    prompt = false
[alias]
    s = status
    l = log --color --decorate --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
	unstage = reset HEAD --
[push]
    default = current
[http]
	postBuffer = 74865667
[filter "lfs"]
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
[init]
	defaultBranch = main
